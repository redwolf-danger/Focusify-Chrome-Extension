  {
    "manifest_version": 3,
    "name": "Focusify",
    "version": "0.0.0.1",
    "background": {
      "service_worker": "bundle.js",
      "type" : "module"
    },
    "author":"advaitasoni1024@gmail.com",
    "description": "Tired of online distractions? Get over your browsing addiction and Increase Productivity!!",
    "action": {
      "default_popup": "/PopUp_Page/hello.html"
    },
    "permissions": ["storage","unlimitedStorage","tabs"],
    "icons":{
      "16" : "/images/16_bit_logo.png",
      "32" : "/images/32_bit_logo.png",
      "48" : "/images/48_bit_logo.png",
      "128" : "/images/128_bit_logo.png"  
    },
    "web_accessible_resources": [
      {
        "resources": ["/images/16_bit_logo.png"],
        "matches": []
      },
      {
        "resources": ["/images/32_bit_logo.png"],
        "matches": []
      },
      {
        "resources": ["/images/48_bit_logo.png"],
        "matches": []
      },
      {
        "resources": ["/images/128_bit_logo.png"],
        "matches": []
      },
      {
        "resources": ["/images/abstract-purple-sunburst-border-frame.jpg"],
        "matches" : []
      },{
        "resources": ["/images/onboarding_image.jpg"],
        "matches" : []
      },{
        "resources": ["/images/site_image.jpg"],
        "matches" : []
      }
    ]
  }